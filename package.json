        "lint": "npx eslint . --ext .ts",
        "prepare": "npm run build"
    },
    "devDependencies": {
        "@types/node": "^20.10.6",
        "@typescript-eslint/eslint-plugin": "^6.16.0",
        "@typescript-eslint/parser": "^6.16.0",
        "eslint": "^8.56.0",
        "prettier": "^3.1.1",
        "typescript": "^5.3.3"
    },
    "dependencies": {
        "chalk": "^4.1.2"
    }
}
