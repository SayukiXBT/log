{
    "name": "@sayukixbt/log",
    "version": "0.0.2",
    "description": "simple logging library",
    "main": "dist/index.js",
    "types": "dist/index.d.ts",
    "author": "SayukiXBT <hello@base32.org>",
    "license": "AGPL-3.0",
    "files": [
        "dist"
    ],
    "keywords": [
        "logger",
        "typescript",
        "console",
        "chalk",
        "colored-logger"
    ],
    "scripts": {
        "start": "npx ts-node ./src/index.ts",
        "build": "node ./node_modules/typescript/bin/tsc",
